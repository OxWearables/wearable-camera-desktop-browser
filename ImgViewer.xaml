    <Grid Height="Auto">
        <MediaElement x:Name="sound_Click_Sound" Source="Click_Sound.wav" LoadedBehavior="Manual" />



        <Rectangle HorizontalAlignment="Stretch" Opacity="0.9" Fill="#FF8A8A8A" x:Name="rect_background" Margin="0,0,0,0" />
        <Border CornerRadius="15" Width="auto" Margin="0,0,0,0" Height="auto" Background="#FFF9FAFC">



            <Grid Background="#FFFFFBEF" >
                <Grid.RowDefinitions>
                    <RowDefinition Height="60" />
                    <RowDefinition Height="*"  />
                    <RowDefinition Height="100" />
                </Grid.RowDefinitions>

                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="250" x:Name="coding_area_column" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="80" />
                </Grid.ColumnDefinitions>

                <!-- close button -->
                <Button Grid.Row="0" Grid.Column="2" x:Name="CloseBtn1" Style="{DynamicResource CloseBtn1}"   RenderTransformOrigin="0.5,0.5"  Click="Close1_Click" HorizontalAlignment="Stretch" IsCancel="True" >
                    <Button.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleX="0.9" ScaleY="0.9"/>
                            <SkewTransform AngleX="0" AngleY="0"/>
                            <RotateTransform Angle="0"/>
                            <TranslateTransform X="7" Y="-10"/>
                        </TransformGroup>
                    </Button.RenderTransform>
                </Button>


                <!-- information and coding area -->
                <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="200" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="140" />
                            <ColumnDefinition Width="120" />
                            <ColumnDefinition Width="120" />
                        </Grid.ColumnDefinitions>

                        <!-- clearing database annotations-->
                        <Button Grid.Column="0" Content="Remove Prior Event Annotations" Height="21" Name="btnCancel_Annotations" VerticalAlignment="Top" HorizontalAlignment="Right" Click="btnCancel_Annotations_Click" />

                        <!-- database annotations -->
                        <!--TextBlock Grid.Column="1" Background="AliceBlue" HorizontalAlignment="Left" x:Name="txt_priorAnnotations" VerticalAlignment="Top" /-->
                        <ListBox Grid.Column="1" Background="AliceBlue" HorizontalAlignment="Left" x:Name="lst_episode_codings_in_database" VerticalAlignment="Top" SelectionMode="Extended" />

                        <TextBlock Grid.Column="2" x:Name="txt_img_time" Text="time" Background="{x:Null}" FontSize="30" TextAlignment="Right" Width="Auto" Padding="0,3,0,0" Height="35" VerticalAlignment="Top" Foreground="#FF0A3B67" />
                        <TextBlock Grid.Column="3" TextWrapping="Wrap" Text="Date" Background="{x:Null}" FontSize="18" TextAlignment="Right" Padding="0,5,0,0" x:Name="txt_img_date" Foreground="#FF0A3B67" />
                        <TextBlock Grid.Column="4" TextWrapping="Wrap" Text="Duration" Background="{x:Null}" FontSize="18" TextAlignment="Right" Padding="0,5,0,0" x:Name="txt_event_length" Foreground="#FF0A3B67" />

                    </Grid>
                </Grid>

                <!-- coding area -->
                <Grid Grid.Row="1" Grid.Column="0">
                    <Grid Grid.Row="1">
                        <Grid.RowDefinitions>
                            <!-- heading -->
                            <RowDefinition Height="40" />
                            <!-- edit annotation/concept types -->
                            <RowDefinition Height="30" />
                            <!-- tree of annotations -->
                            <RowDefinition Height="*" />
                            <!-- record annotation -->
                            <RowDefinition Height="30" />                            
                        </Grid.RowDefinitions>

                        <!-- heading -->
                        <Label Grid.Row="0" Content="ANNOTATION AREA" VerticalAlignment="Bottom" FontSize="16" />

                        <!-- edit annotation/concept types -->
                        <Button Grid.Row="1" x:Name="btnEditConceptType" Content="Edit Event Type List" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Top" Click="btnEditConceptType_Click" />

                        <!-- tree of annotations -->
                        <TreeView Grid.Row="2" x:Name="lst_event_concept_types" ItemsSource="{Binding FirstGeneration}">
                            <TreeView.ItemContainerStyle>
                                <!-- This Style binds a TreeViewItem to an AnnotationViewModel. -->
                                <Style TargetType="{x:Type TreeViewItem}">
                                    <Setter Property="IsExpanded" Value="{Binding IsExpanded, Mode=TwoWay}" />
                                    <Setter Property="IsSelected" Value="{Binding IsSelected, Mode=TwoWay}" />
                                    <Setter Property="FontWeight" Value="Normal" />
                                    <Style.Triggers>
                                        <Trigger Property="IsSelected" Value="True">
                                            <Setter Property="FontWeight" Value="Bold" />
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </TreeView.ItemContainerStyle>

                            <TreeView.ItemTemplate>
                                <HierarchicalDataTemplate ItemsSource="{Binding Children}">
                                    <TextBlock Text="{Binding Name}" />
                                </HierarchicalDataTemplate>
                            </TreeView.ItemTemplate>
                        </TreeView>

                        
                        <!-- record annotation-->
                        <Button Grid.Row="3" x:Name="btnAdd_Annotations" Content="RECORD ANNOTATION" Height="21" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Top" Click="btnAdd_Annotations_Click" />

                    </Grid>

                </Grid>



                <!-- delete images area -->
                <Grid Grid.Row="2" Grid.Column="0">
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>


                    <Button Grid.Row="0" Content="COPY IMAGE TO CLIPBOARD" Height="21" FontSize="14" HorizontalAlignment="Right" Name="btnCopy_Images_to_Clipboard" VerticalAlignment="Center" Click="btnCopy_Images_to_Clipboard_Click" />

                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>


                        <Button Grid.Column="0" HorizontalAlignment="Left" x:Name="btnDelete" Style="{DynamicResource ButtonStyle1}"   RenderTransformOrigin="0.5,0.5" VerticalAlignment="Top" Click="btnDelete_Click">
                            <Button.Resources>
                                <ResourceDictionary>
                                    <ResourceDictionary.MergedDictionaries>
                                        <ResourceDictionary Source="SenseCamDictionary.xaml"/>
                                    </ResourceDictionary.MergedDictionaries>
                                </ResourceDictionary>
                            </Button.Resources>
                            <Button.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX="0.95" ScaleY="1"/>
                                    <SkewTransform AngleX="0" AngleY="0"/>
                                    <RotateTransform Angle="0"/>
                                    <TranslateTransform X="0" Y="0"/>
                                </TransformGroup>
                            </Button.RenderTransform>
                        </Button>

                        <Button Grid.Column="1" HorizontalAlignment="Left" Content="Button" d:LayoutOverrides="HorizontalAlignment" Style="{DynamicResource ButtonStyle5}" x:Name="btnUndo_Delete" Click="btnUndo_Delete_Click"  Visibility="Hidden" VerticalAlignment="Bottom" >
                            <Button.BitmapEffect>
                                <DropShadowBitmapEffect ShadowDepth="2" Softness="0.245"/>
                            </Button.BitmapEffect>
                        </Button>
                    </Grid>
                    

                </Grid>


                <!-- event segmentation control area -->
                <Grid Grid.Row="1" Grid.Column="2">
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>

                    <Button Grid.Row="0" x:Name="btnMerge_With_Previous" Click="btnMerge_With_Previous_Click"  Height="50" >
                        <TextBlock> Put Images in <LineBreak/> <Bold>prev</Bold> evnt</TextBlock>
                    </Button>
                    <Button Grid.Row="1" x:Name="btnSplit_Event" Click="btnSplit_Event_Click"  Height="50" >
                        <TextBlock> <Bold>Split Event</Bold></TextBlock>
                    </Button>
                    <Button Grid.Row="2" x:Name="btnMerge_With_Next" HorizontalAlignment="Left" Click="btnMerge_With_Next_Click"  Height="50" >
                        <TextBlock> Put Images in <LineBreak/> <Bold>next</Bold> event</TextBlock>
                    </Button>

                </Grid>


                <!-- speed and toggle overall view controls area -->
                <Grid Grid.Row="2" Grid.Column="2">
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>

                    <Button Grid.Row="0" Content="Toggle View" HorizontalAlignment="Left" Name="btnToggle_Movie_Wall_View" VerticalAlignment="Top" Click="btnToggle_Movie_Wall_View_Click" />

                    <!-- speed toggle button-->
                    <Grid Grid.Row="1" Grid.Column="1">
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="10" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="10" />
                        </Grid.ColumnDefinitions>

                        <TextBlock Grid.Row="0" Grid.Column="0" VerticalAlignment="Bottom" FontSize="24" Text="-" TextWrapping="Wrap" x:Name="minus_icon"  HorizontalAlignment="Right" MouseLeftButtonDown="minus_icon_MouseLeftButtonDown" />
                        <TextBlock Grid.Row="0" Grid.Column="2" VerticalAlignment="Bottom" FontSize="16" Text="+" TextWrapping="Wrap" x:Name="plus_icon" HorizontalAlignment="Left" MouseLeftButtonDown="plus_icon_MouseLeftButtonDown" />
                        <Slider Grid.Row="0" Grid.Column="1" x:Name="SpeedSlider" PreviewMouseUp="SpeedSlider_PreviewMouseUp"  Orientation="Vertical"  Minimum="0" Maximum="10" Value="5" TickFrequency="1" TickPlacement="BottomRight" SmallChange="1" LargeChange="3" BorderBrush="{x:Null}" Background="#FFEBEBE3" RenderTransformOrigin="0.5,0.5">
                            <Slider.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX="1" ScaleY="2"/>
                                    <SkewTransform AngleX="0" AngleY="0"/>
                                    <RotateTransform Angle="90"/>
                                    <TranslateTransform X="0" Y="20"/>
                                </TransformGroup>
                            </Slider.RenderTransform>
                        </Slider>
                        <TextBlock Grid.Row="1" Grid.Column="1" x:Name="lblSpeed" HorizontalAlignment="Center"  VerticalAlignment="Top" FontSize="12" Text="Speed" TextWrapping="Wrap" />
                    </Grid>
                </Grid>




                <!-- image viewer controls for play pause etc -->
                <Grid Grid.Row="2" Grid.Column="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>

                    <Button Grid.Column="0" Visibility="Hidden" Click="PreviousBtn_Click" HorizontalAlignment="Center" Style="{DynamicResource PreviousBtn}" x:Name="PreviousBtn" VerticalAlignment="Top" />
                    <Button Grid.Column="2" Visibility="Hidden" Click="NextBtn_Click" HorizontalAlignment="Center" Style="{DynamicResource NextBtn}" Content="" x:Name="NextBtn" VerticalAlignment="Top" />
                    <Button Grid.Column="1" Visibility="Hidden"  Click="PlayBtn_Click" Style="{DynamicResource PlayBtn}" Content="" x:Name="PlayBtn" VerticalAlignment="Top" HorizontalAlignment="Center"/>
                    <Button Grid.Column="1" Visibility="Hidden" Click="PlayBtn_Click" x:Name="PauseBtn" Style="{DynamicResource PauseBtn}" VerticalAlignment="Top" Content="Button" HorizontalAlignment="Center" />

                </Grid>


                <!-- image display area-->
                <Grid Grid.Row="1" Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="70" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="40" />
                    </Grid.RowDefinitions>

                    <!-- this area covers image information, e.g. num images, whether they're still loading, and free text caption ... -->
                    <Grid Grid.Row="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="30" />
                            <RowDefinition Height="40" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>

                        <TextBox Grid.Row="1" Grid.Column="0" VerticalAlignment="Bottom" Text="Hello" TextWrapping="Wrap" Margin="0,0,39,3" Height="Auto" FontSize="30" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="#FFFFFFFF" x:Name="txtCaption" GotFocus="txtCaption_Click" >
                            <TextBox.BitmapEffect>
                                <DropShadowBitmapEffect ShadowDepth="3" Softness="0.125" Direction="297"/>
                            </TextBox.BitmapEffect>
                        </TextBox>
                        <TextBlock Grid.Row="0" Grid.Column="1" x:Name="txt_image_number" Padding="3,3,3,3" Margin="0,1.511,21.3,0" Background="{x:Null}" FontSize="18" TextAlignment="Right" HorizontalAlignment="Right" VerticalAlignment="Top" Width="Auto" Height="Auto" Foreground="#FFFFFFFF" ><TextBlock.BitmapEffect>
                        		<DropShadowBitmapEffect Softness="0" Direction="307" ShadowDepth="1"/>
                        	</TextBlock.BitmapEffect><Run Text="Image " /><Run Text="X of Y" Language="en-ie"/></TextBlock>
                        <TextBlock Grid.Row="0" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Center" Text="Please wait a few seconds while the photos are loading..." TextWrapping="Wrap" x:Name="txtPlease_Wait" FontSize="24" Foreground="#FFFAE108" Background="{x:Null}" TextAlignment="Center" Visibility="Visible" Margin="30,0,30,0">
                        	<TextBlock.BitmapEffect>
                        		<DropShadowBitmapEffect ShadowDepth="1" Softness="0" Direction="284"/>
                        	</TextBlock.BitmapEffect>
                        </TextBlock>
                    </Grid>


                    <!-- image or listbox to display items of focus-->
                    <ListBox Grid.Row="1" x:Name="lst_display_images" Style="{StaticResource Display_Images_in_Event}" SelectionMode="Extended"  Margin="0,1,0,0" Visibility="Collapsed" />
                    <Image Grid.Row="1" x:Name="img_to_show" Margin="0,1,0,0" Visibility="Visible" >
                        <Image.OpacityMask>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF000000" Offset="0"/>
                                <GradientStop Color="#FFFFFFFF" Offset="1"/>
                            </LinearGradientBrush>
                        </Image.OpacityMask>
                    </Image>

                    <!-- zoom/play slider-->
                    <Slider Grid.Row="2" x:Name="EventZoomSlider" Margin="10,0,0,0" Orientation="Horizontal" Minimum="80" Maximum="640" Value="160" TickFrequency="80" TickPlacement="BottomRight" SmallChange="5" LargeChange="20" />
                    <Slider Grid.Row="2" x:Name="EventPlaySlider" PreviewMouseUp="EventPlaySlider_PreviewMouseUp"  Visibility="Hidden" Margin="10,0,0,0" Orientation="Horizontal" Minimum="0" Maximum="10" Value="0" TickFrequency="80" TickPlacement="BottomRight" SmallChange="2" LargeChange="20" />


                </Grid>



            </Grid>
        </Border>





        <SCImgViewer:Edit_List_of_Event_Types x:Name="annotation_editor" Visibility="Collapsed" Cursor="Arrow" />
    </Grid>



</UserControl>
