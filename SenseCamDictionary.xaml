 <ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" xmlns:Microsoft_Windows_Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Luna">
  <!-- Resource dictionary entries should be defined here. -->


    <Style x:Key="Display_Images_in_Event" TargetType="{x:Type ListBox}">
        <Setter Property="ItemTemplate" >
            <Setter.Value>
                <!--DataTemplate>
                    <Image Source="{Binding Path=scaled_image_src}" Width="{Binding ElementName=EventZoomSlider, Path='Value'}" Height="{Binding ElementName=EventZoomSlider, Path='Value'}" />
                </DataTemplate-->
                <DataTemplate>
                    <Grid Width="{Binding ElementName=EventZoomSlider, Path='Value'}" Height="{Binding ElementName=EventZoomSlider, Path='Value'}" Margin="5,5,5,5">
                        <Image Margin ="0,0,0,0" Source="{Binding Path=scaled_image_src}" Stretch="Fill">
                            <Image.BitmapEffect>
                                <DropShadowBitmapEffect Softness="0.25" ShadowDepth="2" Direction="315"/>
                            </Image.BitmapEffect>
                        </Image>

                        <StackPanel Margin="0,0,0,0" VerticalAlignment="Bottom" Orientation="Horizontal" Background="#24000000" Opacity="1" >
                            <TextBlock Text="{Binding Path=short_image_time}" FontSize="16"  Margin="5,0,22,0" Foreground="White"/>                            
                        </StackPanel>
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <WrapPanel />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled" />
        <Setter Property="Foreground" Value="White" />
        <Setter Property="Background" Value="White" />
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto" />
    </Style>



    <Style x:Key="Display_Annotations" TargetType="{x:Type ListBox}">
        <Setter Property="ItemTemplate" >
            <Setter.Value>
                <DataTemplate>
                    <Label Content="{Binding Path=annotation_type}" />                                      
                </DataTemplate>
            </Setter.Value>
        </Setter>        
    </Style>



    <Style x:Key="Display_Daily_Activity_Summary" TargetType="{x:Type ListBox}">
        <Setter Property="ItemTemplate" >
            <Setter.Value>
                <DataTemplate>
                    <StackPanel Margin="0,0,0,0" VerticalAlignment="Bottom" Orientation="Horizontal" Background="YellowGreen"  Opacity="0.5" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <Label Grid.Row="0" Content="{Binding Path=annotation_type}" FontFamily="Times New Roman" FontSize="24" HorizontalAlignment="Center" />
                            <Label Grid.Row="1" Content="{Binding Path=daily_duration_string}" FontFamily="Times New Roman" FontSize="16" HorizontalAlignment="Center" />
                        </Grid>
                    </StackPanel>                    
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <WrapPanel Orientation="Horizontal"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Visible" />
        <Setter Property="Foreground" Value="White" />
        <Setter Property="Background" Value="White" />
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Disabled" />        
    </Style>


    <Style x:Key="Daily_Activity_Summary_Highlight_Item" TargetType="{x:Type ListBoxItem}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ListBoxItem">
                    <Border Name="Border" Padding="2" SnapsToDevicePixels="true">
                        <ContentPresenter />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsSelected" Value="true">
                            <Setter TargetName="Border" Property="BorderBrush" Value="Red"/>
                            <Setter Property="Background" TargetName="Border" Value="Red"/>                            
                        </Trigger>
                    </ControlTemplate.Triggers>                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>



    <Style x:Key="Display_Event_Activity_Annotation_List" TargetType="{x:Type ListBox}">
        <Setter Property="ItemTemplate" >
            <Setter.Value>
                <DataTemplate>
                    <StackPanel Margin="0,0,0,0" VerticalAlignment="Bottom" Orientation="Horizontal" Opacity="1" >
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Label Grid.Column="0" Content="{Binding Path=event_index}" FontFamily="Times New Roman" FontSize="10" HorizontalAlignment="Left" />
                            <Label Grid.Column="1" Content="{Binding Path=annotation}" FontFamily="Times New Roman" FontSize="10" HorizontalAlignment="Left" />
                            <Label Grid.Column="2" Content="{Binding Path=duration_string}" FontFamily="Times New Roman" FontSize="10" HorizontalAlignment="Left" />
                        </Grid>
                    </StackPanel>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Foreground" Value="White" />
        <Setter Property="Background" Value="White" />        
    </Style>



    <DataTemplate x:Key="Event_Keyframe_Template">        
            <Grid Width="{Binding ElementName=ZoomSlider, Path='Value'}" Height="{Binding ElementName=ZoomSlider, Path='Value'}" Margin="5,5,5,5" Background="AliceBlue">
                <Image Margin ="0,0,0,0" Source="{Binding Path=keyframe_path}" Stretch="Fill" >
                <Image.BitmapEffect>
                    <DropShadowBitmapEffect Softness="0.25" ShadowDepth="2" Direction="315"/>
                </Image.BitmapEffect>
            </Image>

            <StackPanel Margin="0,0,0,0" VerticalAlignment="Bottom" Orientation="Horizontal" Background="{Binding Path=border_colour}"  Opacity="1" >
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />                        
                    </Grid.RowDefinitions>
                    <TextBlock Grid.Row="0" Text="{Binding Path=str_time}" FontSize="16"  Margin="5,0,22,0" Foreground="White"/>
                    <TextBlock Grid.Row="1" Text="{Binding Path=event_length}" FontSize="16" Foreground="White" Margin="0,0,5,0"/>
                </Grid>                
            </StackPanel>
            </Grid>
        </DataTemplate>

    
    
    <Style x:Key="Events_in_Day" TargetType="ListBox">
        <Setter Property="ItemTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Image Source="{Binding Path=keyframe_path}" Width="{Binding ElementName=ZoomSlider, Path='Value'}" Height="{Binding ElementName=ZoomSlider, Path='Value'}" Margin="5,5,5,5" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <WrapPanel />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled" />
        <Setter Property="Foreground" Value="White" />
        <Setter Property="Background" Value="White" />
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto" />


    </Style>



    <Style x:Key="Users_Listing" TargetType="ComboBox">
        <Setter Property="ItemTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Label Content="{Binding Path=name}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <Style x:Key="string_horizontal_listbox" TargetType="ListBox">
        <Setter Property="ItemTemplate">
            <Setter.Value>
                <DataTemplate>
                    <StackPanel Width="100">
                        <TextBlock Text="{Binding }" HorizontalAlignment="Center" />
                    </StackPanel>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <WrapPanel />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled" />
        <Style.Resources>
            <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="LightGreen"/>
            <SolidColorBrush x:Key="{x:Static SystemColors.ControlBrushKey}" Color="LightBlue"/>
        </Style.Resources>
    </Style>
    

    <Style x:Key="Images_in_Event" TargetType="{x:Type ListBox}">
    <Setter Property="ItemTemplate" >
      <Setter.Value>
        <DataTemplate>
          <Image Source="{Binding Path=scaled_image_src}" Width="215" Height="175" />
        </DataTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="ItemsPanel">
      <Setter.Value>
        <ItemsPanelTemplate>
          <WrapPanel />
        </ItemsPanelTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled" />

  </Style>

 
  
  
